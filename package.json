{
  "name": "gamerarena-frontend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:gamerarena/gamerarena-frontend.git",
  "author": "Gamer Arena <info@gamerarena.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "cross-env GAMERARENA_STAGE=development next dev --port 4001",
    "lint": "next lint",
    "export": "next export",
    "generateSitemap": "node ./scripts/generateSitemap.js",
    "generateRobotsTxt": "node scripts/generateRobotsTxt.js",
    "i18n:check": "node ./scripts/checkLangFiles.js",
    "i18n:compile": "formatjs compile-folder --ast --format simple lang/ compiled-lang/",
    "i18n:extract": "formatjs extract '{components,pages}/**/*.ts*' --format simple --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "i18n:sync": "node scripts/syncLangFiles.js",
    "start": "cross-env NODE_ENV=production next start --port 80",
    "stylelint": "stylelint **/*.css --config ./stylelint.config.js",
    "test": "yarn install && next build && next export && start-server-and-test 'http-server -s -p 8080 out' http://localhost:8080 'cypress run --record --key $CYPRESS_RECORD_KEY --browser chrome'",
    "test:local": "cross-env NODE_ENV=test next build && next export && start-server-and-test 'http-server -s -p 8080 out' http://localhost:8080 'cypress open'"
  },
  "dependencies": {
    "@elastic/apm-rum": "^5.7.2",
    "@formatjs/cli": "^4.2.11",
    "@paypal/react-paypal-js": "^7.1.0",
    "@sentry/nextjs": "^6.3.5",
    "@svgr/webpack": "^5.4.0",
    "@tippyjs/react": "^4.2.0",
    "@widgetbot/react-embed": "^1.4.0",
    "axios": "^0.24.0",
    "babel-plugin-react-intl": "^8.2.3",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "cypress-axe": "^0.10.0",
    "date-fns": "^2.17.0",
    "dayjs": "^1.8.36",
    "dotenv": "^10.0.0",
    "fingerprintjs2": "^2.1.4",
    "firebase": "^8.6.8",
    "focus-visible": "^5.1.0",
    "globby": "^11.0.1",
    "gray-matter": "^4.0.2",
    "lodash": "^4.17.20",
    "markdown-to-jsx": "^7.1.0",
    "next": "canary",
    "postcss-custom-media": "^8.0.0",
    "postcss-flexbugs-fixes": "^5.0.1",
    "postcss-nested": "^5.0.5",
    "postcss-preset-env": "^6.7.0",
    "qs": "^6.9.4",
    "react": "^17.0.2",
    "react-animate-height": "^2.0.23",
    "react-countdown": "^2.3.1",
    "react-cropper": "^2.1.7",
    "react-datepicker": "^4.1.1",
    "react-dom": "^17.0.2",
    "react-intl": "^5.8.2",
    "react-transition-group": "^4.4.1",
    "react-use": "^17.1.1",
    "shield-js-fp": "^1.0.10",
    "swiper": "^6.2.0",
    "swr": "^0.5.3",
    "typescript": "^4.0.3",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/cypress-axe": "^0.8.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^16.0.0",
    "@types/qs": "^6.9.5",
    "@types/react": "^17.0.0",
    "@types/react-datepicker": "^4.1.1",
    "@types/react-transition-group": "^4.4.0",
    "@types/swiper": "^5.4.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "cypress": "5.4.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-next": "^11.0.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "http-server": "^0.12.3",
    "postcss-safe-area": "^0.1.1",
    "source-map-support": "^0.5.19",
    "start-server-and-test": "^1.11.5",
    "stylelint": "^13.7.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-declaration-use-variable": "^1.7.2",
    "stylelint-order": "^4.1.0",
    "stylelint-selector-tag-no-without-class": "^2.0.3",
    "ts-node": "^10.0.0"
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  }
}
